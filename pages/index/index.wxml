


<view class="warp"  data-jump="order" bindtap="jump">
    <view class="uone">
        <image class="uone-img" src="../../imgs/02.png"></image>
        <text class="uone-tone">待处理订单</text>
        <text class="uone-ttwo">{{order.new}}</text>
    </view>

    <view class="utwo">
        <text class="utwo-tone">日订单 : {{income.dayCount}}</text>
        <text class="utwo-tone">月订单 : {{income.monthCount}}</text>
        <text class="utwo-tone">年订单 : {{income.yearCount}}</text>
    </view>
</view>

<view class="warp" data-jump="count" bindtap="jump">
    <view class="uone">
        <image class="uone-img" src="../../imgs/01.png"></image>
        <text class="uone-tone">今日营收</text>
        <text class="uone-ttwo">{{income.dayAmount}}元</text>
    </view>

    <view class="utwo">
        <text class="utwo-tone">月营收 : {{income.monthAmount}}元</text>
        <text class="utwo-tone">年营收 : {{income.yearAmount}}元</text>
    </view>
</view>

<view class="warp" data-jump="data" bindtap="jump">
    <view class="uone">
        <image class="uone-img" src="../../imgs/03.png"></image>
        <text class="uone-tone">小程序数据</text>
        <text class="uone-ttwo"></text>
    </view>

    <view class="utwo">
        <text class="utwo-tone">店铺运营数据, 点击查看 >></text>
    </view>
</view>

<audio src="{{voicePrompt.src}}" id="myAudio"></audio>
