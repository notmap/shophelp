
page {
	background-color: #efefef;
    height: 100%;
    overflow: hidden;
}

button {
    border-radius: 0;
    padding: 0;
}

button:after {
    border: none;
}

.section {
	background: #fff;
	border-bottom: 1rpx solid #dedede;
	margin-bottom: 15rpx;
	padding: 20rpx;
}

.ellipsis {
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}

.container {
    height: 100%;
    overflow: hidden;
}

.placeholder {
    color: #cccccc;
}
